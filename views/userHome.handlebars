                    {{#each homepage}}
                    <li data-id="{{this.id}}" class="list-group-item">
                        <a href="events/{{this.id}}">{{this.title}}</a>
                        <button class="btn btn-danger float-right delete">ï½˜</button>
                        <button class="btn btn-primary float-right invite">+</button>

